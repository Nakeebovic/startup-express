{
  "name": "startup-express",
  "version": "1.0.0",
  "description": "A comprehensive, production-ready Express.js starter package with Zod validation, advanced middleware, and TypeScript-first design",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "express",
    "starter",
    "middleware",
    "boilerplate",
    "express-js",
    "utilities",
    "express-setup",
    "zod",
    "validation",
    "typescript",
    "rest-api",
    "error-handling",
    "request-tracking",
    "production-ready"
  ],
  "author": "Ahmed Nakeeb <nakeebovic>",
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.7",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nakeebovic/startup-express.git"
  },
  "bugs": {
    "url": "https://github.com/Nakeebovic/startup-express/issues"
  },
  "homepage": "https://github.com/Nakeebovic/startup-express#readme"
